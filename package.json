{
  "name": "assembly-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "build:android:preview": "eas build --platform android --profile preview --non-interactive --no-wait",
    "build:all:submit": "eas build --platform all --profile production --auto-submit --no-wait",
    "check:types": "tsc --noEmit",
    "check:lint": "expo lint",
    "check:format": "prettier --check \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "check:test": "jest --bail --ci",
    "check:all": "npm run check:types && npm run check:lint && npm run check:format"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.4",
    "@openspacelabs/react-native-zoomable-view": "^2.3.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "dayjs": "^1.11.13",
    "expo": "^52.0.32",
    "expo-build-properties": "~0.13.2",
    "expo-constants": "~17.0.5",
    "expo-device": "~7.0.2",
    "expo-font": "~13.0.3",
    "expo-linking": "~7.0.5",
    "expo-notifications": "~0.29.13",
    "expo-router": "~4.0.17",
    "expo-splash-screen": "~0.29.21",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.8",
    "expo-tracking-transparency": "~5.1.1",
    "expo-updates": "~0.26.17",
    "expo-web-browser": "~14.0.2",
    "i18next": "^23.16.8",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "^14.1.3",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-pager-view": "6.5.1",
    "react-native-paper": "^5.13.1",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@tsconfig/react-native": "^3.0.5",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.2.79",
    "@types/react-test-renderer": "^18.3.1",
    "eslint": "^8.57.1",
    "eslint-config-expo": "^7.1.2",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.4",
    "prettier": "^3.5.0",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
